public class DetallesPase {

    public Object hora;
    public Object sala;
    public Object pelicula;
    public Date fecha;
    public float precio;
    
    public DetallesPase(Object hora, Object sala, Object pelicula, Date fecha, float precio)
    {
        this.hora = hora;
        this.sala = sala;
        this.pelicula = pelicula;
        this.fecha = fecha;
        this.precio = precio;
    }
}
