    private void botonIdentificarseActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_botonIdentificarseActionPerformed
        String email = textFieldCorreoElectronico.getText();
        String password = textFieldContrasena.getText();
        
        
        usuarioRegistrado = gestorTC.login(email, password);
        
        
        if(usuarioRegistrado != null)
        {
            String tipoUsuario = usuarioRegistrado.getClass().getName();
            System.out.println(tipoUsuario);
            switch(tipoUsuario){
                case "ticketcine.Controlador.Cliente":
                    InicioLogueado inilogin = new InicioLogueado((Cliente) usuarioRegistrado, this.vAnterior);
                    inilogin.setVisible(true);
                    this.setVisible(false);
                    break;
                case "admin":
                    InicioAdmin iniadmin = new InicioAdmin();
                    iniadmin.setVisible(true);
                    this.setVisible(false);
                    break;
                case "ticketcine.Controlador.Gerente":
                    InicioGerente inigerente = new InicioGerente(usuarioRegistrado, this.vAnterior);
                    inigerente.setVisible(true);
                    this.setVisible(false);
                    break;
            }
        }
        else{
            JOptionPane.showMessageDialog(this, "Usuario Invalido. Vuelva a intentarlo");
        }
           
        
    }//GEN-LAST:event_botonIdentificarseActionPerformed
    


