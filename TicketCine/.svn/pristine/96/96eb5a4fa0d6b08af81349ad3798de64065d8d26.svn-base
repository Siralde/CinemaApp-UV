    private void jb_identificarseActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jb_identificarseActionPerformed
        String email = jTextField5.getText();
        String password = jTextField6.getText();
        
        usuarioreg = ticketCine.login(email, password);
        String tipoUsuario = usuarioreg.getClass().getName();
        
        switch(tipoUsuario){
            case "usuario":
                InicioLogueado inilogin = new InicioLogueado((Cliente) usuarioreg);
                inilogin.setVisible(true);
                this.setVisible(false);
                break;
            case "admin":
                InicioAdmin iniadmin = new InicioAdmin();
                iniadmin.setVisible(true);
                this.setVisible(false);
                break;
            case "ticketcine.Controlador.Gerente":
                InicioGerente inigerente = new InicioGerente(usuarioreg);
                inigerente.setVisible(true);
                this.setVisible(false);
                break;
        }
    }//GEN-LAST:event_jb_identificarseActionPerformed
    


