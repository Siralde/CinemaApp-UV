package ticketcine.Controlador;

import java.util.Date;

/**
 *
 * @author Rulos
 */
public class DetallesPelicula {
    public String titulo;
    public String genero;
    public String direccion;
    public int calificacion;
    public String sinopsis;
    public String interpretes;
    public Date fecha_estreno;
    public Object critica;
    
    public DetallesPelicula(String titulo, String genero, String direccion, int calificacion, String sinopsis, String interpretes, Date fecha_estreno, Object critica){
        this.titulo = titulo;
        this.genero = genero;
        this.direccion = direccion;
        this.calificacion = calificacion;
        this.sinopsis = sinopsis;
        this.interpretes = interpretes;
        this.fecha_estreno = fecha_estreno;
        this.critica = critica;
    }
}
